{
    "buildCommand": "npm run build",
    "outputDirectory": "dist",
    "rewrites": [
      { "source": "/:path*", "destination": "/:path*.html" },
      { "source": "/", "destination": "/index.html" }
    ],
    "redirects": [
      { "source": "/index", "destination": "/", "permanent": true }
    ]
}