{
    "buildCommand": "npm run build",
    "outputDirectory": "dist",
    "rewrites": [
        {
            "source": "/about",
            "destination": "/about.html"
        },
        {
            "source": "/",
            "destination": "/index.html"
        }, 
        { 
            "source": "/:path*", 
            "destination": "/:path*" 
        }
    ],
    "redirects": [
        {
            "source": "/index",
            "destination": "/",
            "permanent": true
        }
    ]
}